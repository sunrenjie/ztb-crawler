   <script>
function search()
		{
			var tim=10
			if(tim==10)
			{
				tim=0;
				if(document.all.txtkey.value=="")
				{
					alert("搜索字段不能为空！");
				}
				else
				{
					var openUrl='/njhw/showinfo/searchresult.aspx?keyword=';
					openUrl+=escape(document.all.txtkey.value);
				
					openUrl+="&categoryNum="+document.all.categoryNum.value;
					window.open(openUrl);
				}
			}
			else
		    {
				alert("两次搜索的时间间隔不能少于十秒")
		    }
		}
		
		function CheckSearch()
		{
			if(event.keyCode=="13")
			{   
				if(document.all.txtkey.value!="")
				{
					search();
				}
				else
				{
					alert("搜索字段不能为空！");
				}
			}
		}
		function Clear()
        {
        document.all.txtkey.value=""
        }
    </script>
