<script language="JavaScript"> 
   function Clear() {
        document.all.txtKeys.value = ""
    }
function search1() {
        if (document.all.txtKeys.value == "" || document.all.txtKeys.value == "请输入搜索内容") {
            alert("搜索字段不能为空！");
        }
        else {
			var t = document.getElementById('txtKeys').value;
           if (t.indexOf('<') > -1 || t.indexOf('>') > -1) {
             alert('请勿在关键字中输入非法字符!');
              return ; }
            var openUrl = "/yxweb/ShowInfo/SearchResult.aspx?keyword=";
            openUrl += escape(document.all.txtKeys.value);
            for (var i = 0; i < document.all.searchtype.length; i++) {
                if (document.all.searchtype.options[i].selected)
                    openUrl += '&searchtype=' + document.all.searchtype.options[i].value;
            }

            window.open(openUrl);
        }

    }
   
             </script>

