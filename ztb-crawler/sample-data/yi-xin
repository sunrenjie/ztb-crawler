<script type="text/javascript"> 
 $(document).ready(function () {
        var $img = $("#moreinfopage img");

 
        $.each($img, function (i) {
            var imgwidth = $(this).width();
            var imgheight = $(this).height();
            var bl = imgwidth / imgheight;
            var height1 = 720 / bl;
            if (imgwidth > 720) {

                $(this).css("width", "720px");
                $(this).css("height", height1 + "px");
            }
            else {
                $(this).css("width", imgwidth);
                $(this).css("height", imgheight);
            }

        })


    });
  $(function () {
       
        equelHeight();
        
    })
  function equelHeight() {
		
     
        var $leftTable = $("#left_table");
        var $rightTable = $("#right_table");
        var leftheight = $leftTable.height();
        var rightheight = $rightTable.height();
	
        if (leftheight >= rightheight-331) {
            $rightTable.css("height", (leftheight+331) + "px");

        }
        else {
            $leftTable.css("height", (rightheight-331)+ "px");

        }
    }
</script>
